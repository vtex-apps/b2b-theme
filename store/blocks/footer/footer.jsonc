    "children": ["link#facebook", "link#instagram", "link#twitter"],
    "props": {
      "blockClass": "networks",
      "horizontalAlign": "center",
      "preserveLayoutOnMobile": true,
      "preventHorizontalStretch": true,
      "colGap": 3,
      "colSizing": "auto",
      "fullWidth": false
    }
  },
  "link#facebook": {
    "children": ["image#facebook"],
    "props": {
      "href": "//facebook.com",
      "target": "_blank",
      "blockClass": "networkLink"
    }
  },
  "link#instagram": {
    "children": ["image#instagram"],
    "props": {
      "href": "//instagram.com",
      "target": "_blank",
      "blockClass": "networkLink"
    }
  },
  "link#twitter": {
    "children": ["image#twitter"],
    "props": {
      "href": "//twitter.com",
      "target": "_blank",
      "blockClass": "networkLink"
    }
